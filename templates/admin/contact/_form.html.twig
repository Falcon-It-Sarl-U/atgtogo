<div class="col-md-12">
	<div class="card">
		<div class="card-header">
			<h5 class="card-title">Equipes</h5>
			<h6 class="card-subtitle text-muted">Tous les champs sont obligatoires</h6>
		</div>
		<div class="card-body">
			{{ form_start(form) }}
			<div class="row">
				<div class="mb-3 col-md-6">
					{{ form_row(form.aproposCompagnie) }}
				</div>
				<div class="mb-3 col-md-6"> {{ form_row(form.adresse) }}</div>
			</div>
            <div class="row">
				<div class="mb-3 col-md-6">
					{{ form_row(form.telephone) }}
				</div>
				<div class="mb-3 col-md-6">{{ form_row(form.email) }}</div>
			</div>
            <div class="row">
				<div class="mb-3 col-md-6">
					{{ form_row(form.email2) }}
				</div>
			</div>
        
			<button type="submit" class="btn btn-primary">Enregistrer</button>
			{{ form_end(form) }}
		</div>
	</div>
</div>
